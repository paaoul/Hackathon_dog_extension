{
  "name": "Dog Extension",
  "version": "1.0",
  "description": "This does something",
  "background": {
    "service_worker": "main.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "pictures/dog.jpeg",
        "pictures/dog-hop2.png",
        "pictures/dog-midwalk.png",
        "pictures/bark.mp3",
        "pictures/bark1.mp3",
        "pictures/bark2.mp3"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": ["scripting", "activeTab", "offscreen"],
  "host_permissions": ["<all_urls>"],
  "manifest_version": 3
}
